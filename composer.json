{
    "name": "sharapov/DoctrineExtensions",
    "type": "library",
    "description": "A batch of Doctrine2 extensions",
    "keywords": ["doctrine", "orm", "database"],
    "license": "LGPL",
    "authors": [
        {"name": "Brandon Turner", "email": "bturner@bltweb.net"},
        {"name": "Alexander Sharapov", "email": "alexander@sharapov.biz"}
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "doctrine/orm": "~2.1"
    },
    "suggest": {
        "doctrine/orm": "Doctrine ORM model"
    },
    "autoload": {
        "psr-4": {"DoctrineExtensions\\" : "src/"}
    },
    "autoload-dev": {
        "psr-4": { "DoctrineExtensions\\Tests\\": "tests/" }
    },
    "extra": {

    }
}
